{
    "family": "resume",
    "executionRoleArn": "arn:aws:iam::267543455085:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "cv",
            "image": "267543455085.dkr.ecr.eu-central-1.amazonaws.com/cv:latest",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}